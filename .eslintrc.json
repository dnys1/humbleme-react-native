{
  "env": {
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": [0],
    "react/require-default-props": [0],
    "global-require": [0],

    "react/prefer-stateless-function": [0],
    "react/no-multi-comp": [0],
    "react/prop-types": [0]
  }
}
